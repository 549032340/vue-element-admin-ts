{
  "name": "vue-element-admin-ts",
  "version": "0.1.1",
  "private": true,
  "description": "A front-end template built on vue-cli3 and typescript",
  "author": "liwb <liwb@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "deploy": "npm run build && node build/zip.ts",
    "analyz": "vue-cli-service build --mode analyz",
    "report": "npm_config_generate_report=true npm run build",
    "test": "",
    "svg": "vsvg -s ./src/icons/svg -t ./src/icons/components --ext ts --es6",
    "release": "sh ./build/release.sh"
  },
  "dependencies": {
    "@liwb/cloud-utils": "^1.3.0",
    "@types/echarts": "^4.1.4",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^10.12.18",
    "@types/nprogress": "^0.0.29",
    "@types/screenfull": "^3.3.2",
    "@types/store": "^2.0.1",
    "@types/webpack-env": "^1.13.6",
    "axios": "^0.18.0",
    "driver.js": "^0.9.3",
    "echarts": "^4.1.0",
    "element-ui": "^2.5.3",
    "fuse.js": "^3.4.2",
    "magicless": "^1.1.0",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "path-to-regexp": "^3.0.0",
    "register-service-worker": "^1.5.2",
    "screenfull": "^4.0.0",
    "store": "^2.0.12",
    "vue": "^2.5.21",
    "vue-class-component": "^6.0.0",
    "vue-count-to": "^1.0.13",
    "vue-i18n": "^8.8.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.2",
    "vue-svgicon": "^3.2.2",
    "vuex": "^3.1.0",
    "vuex-class": "^0.3.1",
    "vuex-module-decorators": "^0.9.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-plugin-typescript": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "archiver": "^3.0.0",
    "chalk": "^2.4.2",
    "compression-webpack-plugin": "^2.0.0",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "husky": "^1.3.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "runjs": "^4.3.3",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "typescript": "^3.2.2",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.5.21",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "vue-cli3",
    "vue",
    "typescript",
    "admin",
    "element-ui-admin",
    "axios"
  ],
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts-info": {
    "serve": "运行开发服务器",
    "build": "生产环境执行构建",
    "analyz": "生产环境执行构建打包分析",
    "deploy": "生产环境执行构建并压缩zip包"
  }
}
